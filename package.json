{
  "name": "shoten-search",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "build": "routify -b && rollup -c && spank -p 0",
    "now-build": "routify -b && rollup -c",
    "serve": "spassr --ssr",
    "export": "routify export",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^9.8.6",
    "carbon-components-svelte": "^0.15.0",
    "carbon-icons-svelte": "^10.21.0",
    "cssnano": "^4.1.10",
    "nollup": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.18.1",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte-hot": "^0.10.0",
    "rollup-plugin-terser": "^6.1.0",
    "spassr": "^2.1.3",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.5.1"
  },
  "dependencies": {
    "@carbon/themes": "^10.23.0",
    "@roxi/routify": "^2.7.3",
    "carbon-components": "^10.24.0",
    "fs-extra": "^9.0.1",
    "moment": "^2.29.1",
    "postcss-import": "^12.0.1",
    "sass": "^1.29.0",
    "spank": "^1.4.0",
    "svelte": "^3.29.6",
    "svelte-device-detector": "^0.2.1",
    "svelte-grid": "^2.3.0",
    "svelte-heatmap": "^1.0.2"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify"
  },
  "spank": {
    "blacklist": []
  },
  "license": "CC0-1.0"
}
